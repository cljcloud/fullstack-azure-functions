{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxy-assets": {
      "debug": true,
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "assets/{*path}"
      },
      "backendUri": "{proxy-assets}"
    },
    "favicon": {
      "debug": true,
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "favicon.ico"
      },
      "backendUri": "{proxy-favicon}"
    }
  }
}